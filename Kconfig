source "arch/Kconfig"

menu "Compile-time checks and compiler options"

config DEBUG
	bool "Debug compile option"
	default y

config GCC_V
	bool "GCC option v"
	default n

endmenu

menu "VM configuration"

config NR_VMS
	int "Number of Virtual Machines(or Guest OS)"
	default 1
	range 1 256

config VA_START
	hex "Virtual address of guest OS"
	default 0x00000000

endmenu
